{
	"name": "@mheob/changeset-changelog",
	"version": "3.0.2",
	"description": "My personal changeset changelog configuration.",
	"keywords": [
		"changelog",
		"changeset",
		"config"
	],
	"homepage": "https://github.com/mheob/changeset-changelog",
	"bugs": "https://github.com/mheob/changeset-changelog/issues",
	"repository": {
		"type": "git",
		"url": "https://github.com/mheob/changeset-changelog"
	},
	"license": "MIT",
	"author": "Alexander BÃ¶hm <tools@boehm.work>",
	"exports": {
		".": {
			"import": "./dist/index.mjs",
			"require": "./dist/index.js"
		},
		"./package.json": "./package.json"
	},
	"main": "dist/index.js",
	"module": "dist/index.mjs",
	"files": [
		"dist"
	],
	"scripts": {
		"build": "tsdown",
		"changeset": "changeset",
		"dev": "tsdown --watch",
		"lint": "eslint . --fix",
		"prepare": "tsx ./scripts/prepare.ts",
		"release": "changeset publish",
		"test": "vitest",
		"test:cov": "vitest run --coverage",
		"test:run": "vitest run",
		"test:ui": "vitest --ui",
		"version-packages": "changeset version && pnpm run lint"
	},
	"lint-staged": {
		"*.{cjs,js,mjs,ts}": "eslint --fix",
		"package.json": "pnpm dlx sort-package-json"
	},
	"dependencies": {
		"@changesets/get-github-info": "^0.6.0",
		"@changesets/types": "^6.1.0",
		"dotenv": "^16.6.1"
	},
	"devDependencies": {
		"@changesets/cli": "^2.29.7",
		"@changesets/parse": "^0.4.1",
		"@commitlint/cli": "^19.8.1",
		"@mheob/commitlint-config": "^1.2.5",
		"@mheob/eslint-config": "^8.14.5",
		"@mheob/prettier-config": "^3.4.0",
		"@mheob/tsconfig": "^2.2.1",
		"@types/node": "^22.19.1",
		"@vitest/coverage-v8": "^3.2.4",
		"commitizen": "^4.3.1",
		"cz-git": "^1.12.0",
		"eslint": "^9.39.1",
		"husky": "^9.1.7",
		"lint-staged": "^16.2.7",
		"prettier": "^3.6.2",
		"tsdown": "^0.16.6",
		"tsx": "^4.20.6",
		"typescript": "^5.9.3",
		"vite": "^6.3.5",
		"vitest": "^3.2.4"
	},
	"packageManager": "pnpm@10.22.0",
	"engines": {
		"node": "^22.21.1"
	},
	"pnpm": {
		"onlyBuiltDependencies": [
			"esbuild",
			"unrs-resolver"
		]
	},
	"tsdown": {
		"clean": true,
		"entry": [
			"src/index.ts"
		],
		"format": [
			"esm",
			"cjs"
		]
	}
}
